        </div>
      </div>
      <!--Navgation End-->

      <div class="banner">
        
      </div>
      <!--banner End-->

      <article>
        <aside class="width67">
          <div class="module video-module">
            <div class="video"> 
            <input type="hidden" name="video_val" id="video_val" value="{{news.video}}">            
             <div id="myElement" >Loading the player...</div>
            </div>            
            <p style="padding-top:10px;">{{ news.description }}</p>
           
            <div class="video-description">
              <span class="uploaded-date"><script> document.write(epochToDate({{ news.uploaded_date }})) </script></span>
            </div>
          </div>
          <div class="clear"></div>
          <div class="module">
          </div>
          <div class="gallery-module">
                <div class="module-title">More Videos</div>
                <div>
                  {% for vid in trendingvideos %}
                  <div class="gallery">
                    <div class="videothumb">
                      <a href="/v/{{vid.id}}">
                        <img width="100%" src="http://newscdn.lycos.com/world_news/{{vid.thumb_image}}" alt="{{vid.title}}"/>
                        <div class="video-overlay">
                          <span class="uploadedDate">{{vid.upload_date}}</span>
                          <span class="duration">{{vid.duration}}</span>
                        </div>
                        <img class="play" src="/images/play-icon.png" alt="Play"/>
                      </a>
                    </div>
                    <a class="video-title" href="/v/{{vid.id}}">{{vid.title}}</a>
                  </div>
                  {% endfor %}
                </div>
              </div>
              <span class="clearboth"></span>
              <span class="clearboth"></span>
              <!-- <div class="row">
                <pagination on-select-page="pageChanged(page)" items-per-page="videosPerPage" total-items="bigTotalItems" page="bigCurrentPage" max-size="maxSize" boundary-links="true" rotate="false" num-pages="numPages"></pagination>
              </div> -->
